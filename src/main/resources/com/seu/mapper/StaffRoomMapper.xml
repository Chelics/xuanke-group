<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seu.mapper.StaffRoomMapper">
    <!--    条件查询-->
    <select id="list" resultType="com.seu.pojo.Room">
        select *
        from room
        <where>
            <if test="building!=null and building!=''">building like concat('%', #{building}, '%')</if>
            <if test="roomName!=null and roomName!=''">room_name like concat(#{roomName}, '%')</if>
            <if test="begin!=null and end!=null">and room_storage between #{begin} and #{end}</if>
        </where>
        order by room_name
    </select>

</mapper>